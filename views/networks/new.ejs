<%- include('../partials/header') %>

<h1>/n</h1>

<form action="/networks" enctype="multipart/form-data" method="POST">
    <label>Name</label>
    <input type="text" name="name" required>
    <label>Email</label>
    <input type="text" name="email" >
    <label>Phone</label>
    <input type="text" name="phone" >
    <input type="file" name="image">
    <a href="/networks" class="button">Cancel</a>
    <input type="reset" value="Clear">
    <input type="submit" value="Save to Network">
</form>


